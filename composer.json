{
  "name": "morganestes/system-report",
  "description": "WordPress plugin for displaying system info.",
  "version": "0.1.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "wordpress-plugin",
  "homepage": "https://github.com/morganestes/system-report",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Morgan Estes",
      "email": "morgan.estes@gmail.com"
    }
  ],
  "extra": {
    "installer-paths": {
      "includes/{$name}/": [
        "stevegrunwell/wp-cache-remember"
      ]
    }
  },
  "require": {
    "stevegrunwell/wp-cache-remember": "^1.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "v0.4.4",
    "wimg/php-compatibility": "dev-master",
    "wp-coding-standards/wpcs": "0.14.1"
  },
  "autoload": {
    "psr-4": {
      "MorganEstes\\SystemReport\\": [
        "src/"
      ]
    },
    "files": [
      "src/functions.php"
    ]
  }
}
